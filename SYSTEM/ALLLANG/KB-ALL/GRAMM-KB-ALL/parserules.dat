(ADV   interj 
 	(after
 	  (PREP ((head-word-typ &greetings)
 	         (word-typ &indobj-prep))
 	         PREP-RMOD))
 	(before
 	  (ADJ ((head-word-typ &greetings)
 	        (dep-follows art))
 	         ADJC-RMOD)))

